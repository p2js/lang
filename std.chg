mod intrinsics;
mod libc;
mod mem;
mod ptr;
mod str;
mod int;
mod vec;
mod cstr;

fn print(s: str*) {
    let parts = str::StrRepr::from(s);
    libc::write(1, ptr::cast(parts.ptr), parts.len);
}

fn println(s: str*) {
  print(s);
  print("\n");
}

fn fatal(s: str*) {
  println(s);
  libc::abort();
}
